<div class="dashboard-container">
    <header>
        Dashboard
    </header>

    <section class="cashier-infor">
        <img src="" alt="" class="cashier-image" />
        <div class="col1">
            <div>
                {{user.UserName}}
            </div>
            <div>
                Tuổi:
                {{age}}
            </div>
            <div>
                Giới tính:
                {{user.GioiTinh}}
            </div>
            <div>
                Chức vụ:
                {{role}}
            </div>

        </div>
        <div class="col2">
            <div><i class="bx bx-phone"></i>Điện thoại:
                {{user.SDT}}</div>
            <div><i class="bx bx-envelope"></i>Email:
                {{user.Email}}</div>
            <div><i class="bx bx-target-lock"></i>Địa chỉ:
                {{user.DiaChi}}</div>
        </div>
        <button class="btn-edit">
            Chỉnh sửa
        </button>
    </section>

    <div class="state-container">
        <div class="newest-order">
            <div class="dashboard-sub-header">
                Hóa đơn mới nhất
            </div>

            <section >
                <table class="newest-order-table">
                    <thead>
                        <tr>
                            <th>#</th>
                            <th>Mã hóa đơn</th>
                            <th>Phương thức</th>
                            <th>Tình trạng</th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody id="billtbody">
                    
                    </tbody>
                </table>
                <i class="bx bx-expand-alt btn-expand"></i>
            </section>
        </div>

        <div class="newest-customer">
            <div class="dashboard-sub-header">
                Khách hàng đặt hàng gần nhất

            </div>

            <section id="customerinfo">
                
            </section>
        </div>
    </div>
</div>