<div id="edit" onclick="offEdit()">
  {{>edit_profile}}
</div>

<div class="profile">
    <header>
        Profile
    </header>

    <section class="phead">
        <img src="" alt="" class="avatar" />
        <div class="col1">
            <div class="name">
                {{user.HoTen}}
            </div>
            <div>
                {{user.UserName}}
            </div>
        </div>
    </section>
    <div class="detail_pro">
        <div class="d_title">
            <h1>Thông tin</h1>
            <div><i class='bx bxs-edit-alt icon' onclick="onEdit()" ></i></div>
        </div>
        <div   class="detail" >
            <p>Họ và tên: {{user.HoTen}}</p>
            <p>Giới tính: {{user.GioiTinh}}</p>
            <p>Ngày sinh: {{user.NgaySinh}}</p>
            <p>Tên đăng nhập: {{user.UserName}}</p>
            <p>Email: {{user.Email}}</p>
            <p>SDT: {{user.SDT}}</p>

        </div>

    </div>

</div>
<script src="/js/edit_profile.js"></script>